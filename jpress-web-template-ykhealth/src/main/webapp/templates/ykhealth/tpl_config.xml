<?xml version="1.0" encoding="UTF-8"?>
<config>
	<infos>
		<title>YkHealth</title>
		<id>ykHealth</id>
		<description>JBlog是JPress内置的博客模板，后续会持续升级。</description>
		<author>jpress</author>
		<authorWebsite>http://www.jpress.io</authorWebsite>
		<version>v1.0</version>
		<versionCode>1</versionCode>
		<updateUrl>http://www.jpress.io</updateUrl>
	</infos>

	<module title="日常记录" name="daily" list="所有诊疗记录" add="撰写诊疗记录" comment="评论">
		<taxonomy title="接诊分类" name="category" formType="select" />
		<taxonomy title="接诊标签" name="tag" formType="input" />
	</module>

	<module title="成功案例" name="case" list="所有案例" add="撰写案例" comment="评论">
		<taxonomy title="案例分类" name="category" formType="select" />
		<taxonomy title="专题" name="feature" formType="select" >
			<metadata dataType="input" name="author" title="专题作者"  description="这个数据只是模板自定义模型的测试。" placeholder="请填写作者用户名"/>
		</taxonomy>
		<taxonomy title="标签" name="tag" formType="input" />
		
		<metadata dataType="input" name="_meta1" title="缩略图1"  placeholder="附加缩略图1"/>
		<metadata dataType="input" name="_meta2" title="缩略图2"  placeholder="附加缩略图2"/>
	</module>

	<module title="百科常识" name="wikipedia" list="所有百科" add="撰写百科" comment="评论">
		<taxonomy title="百科分类" name="category" formType="select" />
		<taxonomy title="百科标签" name="tag" formType="input" />
	</module>

	<module title="页面" name="page" list="所有页面" add="新建页面" comment="评论">
	</module>

	<thumbnail name="t1" size="780 x 240" />
	<thumbnail name="t2" size="240 x 140" />
	<thumbnail name="t3" size="600 x 300" />
	<thumbnail name="t4" size="300 x 300" />

</config>